>>> import numpy as np
>>> import pandas as pd

>>> s = pd.Series(np.random.randn(5), index=['a', 'b', 'c', 'd', 'e'])
>>> s
a    0.383621
b    0.472118
c    0.286304
d    0.215361
e   -0.642872
dtype: float64
>>> s.index
Index(['a', 'b', 'c', 'd', 'e'], dtype='object')

